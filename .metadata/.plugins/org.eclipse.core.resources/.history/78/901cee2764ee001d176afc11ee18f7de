package array2;

public class Array1D {
	public static int max(int arr[])
	{
		int max=0;
		max=arr[0];
		for(int i=0;i<arr.length;i++)
		{
			if(max<arr[i])
				max=arr[i];
			
		}
		return max;
	}

	public static void main(String[] args) {
		int arr[]= {12,13,14,15,16,17,18,19,20};
		System.out.println("Max:"+max(arr));
		

	}

}
